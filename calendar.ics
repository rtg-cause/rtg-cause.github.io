---
layout: none
permalink: /assets/calendar.ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//CAUSE//Calendar//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{% for event in site.calendar %}
BEGIN:VEVENT
DTSTAMP:{{ "now" | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ event.start_date | date: "%Y%m%dT%H%M%SZ" }}
DTEND:{{ event.end_date | date: "%Y%m%dT%H%M%SZ" }}
SUMMARY:{{ event.title | strip_newlines }}
DESCRIPTION:{{ event.description | strip_newlines | escape }}
LOCATION:{{ event.location | strip_newlines | escape }}
{% if event.data.url %}
UID:{{ event.data.url | absolute_url }}
URL:{{ event.data.url | absolute_url}}
{% endif %}
END:VEVENT
{% endfor %}
END:VCALENDAR
