---
layout: none
permalink: /assets/calendar.ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//CAUSE//Calendar//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{% for event in site.calendar %}
BEGIN:VEVENT
DTSTAMP:{{ "now" | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ event.start_date | date: "%Y%m%dT%H%M%S" }}
DTEND:{{ event.end_date | date: "%Y%m%dT%H%M%S" }}
SUMMARY:CAUSE - {{ event.title | strip_newlines }}
DESCRIPTION:CAUSE 
 {{ event.description | strip_newlines | escape }}
LOCATION:{% include chunk.ics string=event.location n=66 %}
{% if event.data.url %}
UID:{{ eventurl | absolute_url }}
URL:{{ eventurl | absolute_url}}
{% endif %}
{% if event.reminder %}
BEGIN:VALARM
ACTION:DISPLAY
TRIGGER:-P{{ event.reminder }}
DESCRIPTION:Reminder for CAUSE Event
END:VALARM
{% endif %}
END:VEVENT
{% endfor %}
END:VCALENDAR
